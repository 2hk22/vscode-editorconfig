{
    "name":	 "editorconfig",
    "publisher": "vscode",
    "activationEvents": ["workspaceContains:.editorconfig"],
    "version": "0.1.0",
    "engines": { "vscode": "*" },
    "main": "./out/editorConfigMain",
    "contributes": {
        "commands": [{
            "command": "vscode.editorconfig",
            "title": "EditorConfig: Start"
        },
        {
            "command": "vscode.generateeditorconfig",
            "title": "EditorConfig: Generate"
        }]
    },
    "dependencies": {
        "editorconfig": "*"
    },
    "devDependencies": {
        "vscode": "*"
    },
    "scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	}
}